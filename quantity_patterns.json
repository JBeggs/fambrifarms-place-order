  "command_patterns": {
    "add_to_order": "please\\s+add\\s+(.+?)\\s+to\\s+(.+?)(?:\\s+boxes?)?$",
    "image_placeholder": "<image:([^>]+)>",
    "whatsapp_message": "^\\[(?<ts>[^\\]]+)\\]\\s*(?<company>[^→·]*?)\\s*(?:·\\s*(?<phone>[^→]+))?\\s*→\\s*(?<text>.*)$",
    "normalization": "[^a-z0-9]"
  },
  "quantity_corrections": [
    {
      "pattern": "(\\d+)([×x])(\\d+)(kg|g)([A-Za-z])",
      "replacement": "$1$2$3$4 $5",
      "description": "Fix spacing in complex patterns like '2×5kgTomato' → '2×5kg Tomato'"
    },
    {
      "pattern": "(\\d+)(kg|g)([A-Za-z])",
      "replacement": "$1$2 $3",
      "description": "Fix spacing in weight patterns like '10kgOnions' → '10kg Onions'"
    },
    {
      "pattern": "(\\d+)([×x])([A-Za-z])",
      "replacement": "$1$2 $3",
      "description": "Fix spacing in multiplication patterns like '3xBroccoli' → '3x Broccoli'"
    },
    {
      "pattern": "(\\d+)(box|boxes|bag|bags|pun|pkt|pkts|bunch|bunches|head|heads)([A-Za-z])",
      "replacement": "$1$2 $3",
      "description": "Fix spacing in container patterns like '5boxTomatoes' → '5box Tomatoes'"
    }
  ]
